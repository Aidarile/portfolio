{
  "version": 2,
  "builds": [
    {
      "src": "dist/aidagarmer/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/dist/aidagarmer/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/aidagarmer/index.html"
    }
  ]
}