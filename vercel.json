{
  "version": 2,
  "buildCommand": "ng build",
  "outputDirectory": "dist/aidagarmer",
  "framework": null,
  "installCommand": "npm ci",
  "routes": [
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|svg|gif|woff|woff2|ttf|eot))",
      "headers": { "Cache-Control": "public, max-age=31536000" },
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}